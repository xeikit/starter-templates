{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rule-check": "npx @eslint/config-inspector",
    "lint:ci": "eslint .",
    "typecheck": "nuxi typecheck .",
    "test": "vitest --run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@hono/zod-validator": "0.7.3",
    "@pinia/nuxt": "0.11.2",
    "consola": "3.4.2",
    "hono": "4.9.8",
    "nuxt": "4.1.2",
    "pinia": "3.0.3",
    "vue": "3.5.22",
    "vue-router": "4.5.1",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.3.0",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@pinia/testing": "1.0.2",
    "@typescript-eslint/parser": "8.44.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.13",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.36.0",
    "eslint-plugin-xeiculy": "1.2.1",
    "globals": "16.4.0",
    "happy-dom": "18.0.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.8"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,cts,mts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.json": "prettier --write",
    "**/*.{ts,cts,mts,vue}": "nuxi typecheck ."
  }
}
