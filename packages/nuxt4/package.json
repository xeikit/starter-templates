{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest run --coverage",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "eslint:ci": "eslint . --max-warnings 0",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "lint": "npm run eslint && npm run prettier && npm run biome && npm run typecheck",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix && npm run biome:fix && npm run typecheck",
    "typecheck": "nuxt typecheck .",
    "prepare": "jiti .husky/install.mts"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "consola": "^3.4.2",
    "hono": "^4.11.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/test-utils": "^3.21.0",
    "@pinia/nuxt": "^0.11.3",
    "@pinia/testing": "^1.0.3",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "9.39.2",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "globals": "16.5.0",
    "happy-dom": "20.0.11",
    "husky": "9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "16.2.7",
    "nuxt": "^4.2.2",
    "pinia": "^3.0.4",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.1.8"
  }
}
