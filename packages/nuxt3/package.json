{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rule-check": "npx @eslint/config-inspector",
    "typecheck": "nuxi typecheck .",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@pinia/nuxt": "0.11.1",
    "nuxt": "3.17.5",
    "pinia": "3.0.3",
    "vue": "3.5.16",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.1.0",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/test-utils": "3.19.1",
    "@pinia/testing": "1.0.2",
    "@typescript-eslint/parser": "8.34.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.2.7",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.29.0",
    "eslint-plugin-xeiculy": "1.2.1",
    "globals": "16.2.0",
    "happy-dom": "17.6.3",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,cts,mts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.json": "prettier --write",
    "**/*.{ts,cts,mts,vue}": "nuxi typecheck ."
  }
}
