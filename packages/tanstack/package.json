{
  "name": "tanstack-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm biome && pnpm prettier && pnpm typecheck",
    "lint:fix": "pnpm biome:fix && pnpm prettier:fix && pnpm typecheck",
    "test": "vitest --run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-router": "1.122.0",
    "@tanstack/react-start": "1.122.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "vite": "6.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/vite": "4.1.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "17.6.3",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "**/*.{css,js,ts,cjs,mjs,cts,mts,jsx,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "**/*.{md,html,yaml,yml,json,jsonc}": [
      "prettier --write"
    ]
  }
}
