{
  "name": "frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "format": "pnpm prettier && pnpm biome",
    "format:fix": "pnpm prettier:fix && pnpm biome:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "eslint:ci": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm eslint && pnpm biome && pnpm prettier && pnpm typecheck",
    "lint:fix": "pnpm eslint:fix && pnpm biome:fix && pnpm prettier:fix && pnpm typecheck",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@tanstack/react-router": "1.132.7",
    "@tanstack/react-start": "1.132.9",
    "@vitejs/plugin-react": "5.0.3",
    "consola": "3.4.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "vite": "7.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@eslint/js": "9.36.0",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.36.0",
    "eslint-plugin-tailwindcss": "3.18.2",
    "globals": "16.4.0",
    "happy-dom": "18.0.1",
    "husky": "9.1.7",
    "jiti": "2.6.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "lint-staged": {
    "**/*.{js,ts,cjs,mjs,cts,mts,jsx,tsx}": [
      "biome check --write --no-errors-on-unmatched",
      "eslint --fix"
    ],
    "**/*.{css,scss,sass,less,md,html,yaml,yml,json,jsonc}": [
      "prettier --write"
    ]
  }
}
