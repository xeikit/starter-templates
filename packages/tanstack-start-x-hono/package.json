{
  "name": "cf-sample-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm --recursive run dev",
    "dev:frontend": "pnpm --filter ./apps/frontend dev",
    "dev:backend": "pnpm --filter ./apps/backend dev",
    "build": "pnpm --filter ./apps/frontend build",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "eslint:ci": "eslint . --max-warnings 0",
    "eslint:inspect": "pnpm dlx @eslint/config-inspector@latest",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "typecheck": "pnpm --recursive run typecheck",
    "lint": "pnpm eslint && pnpm biome && pnpm prettier",
    "lint:fix": "pnpm eslint:fix && pnpm biome:fix && pnpm prettier:fix",
    "prepare": "jiti .husky/install.mts"
  },
  "packageManager": "pnpm@10.26.0",
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.26.0"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.26.0",
    "npm": "Please use pnpm"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@commitlint/cli": "20.1.0",
    "@commitlint/types": "20.0.0",
    "commitlint-config-gitmoji": "2.3.1",
    "consola": "3.4.2",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0"
  }
}
