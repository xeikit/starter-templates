{
  "name": "react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:ci": "biome ci .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check --log-level warn .",
    "typecheck": "react-router typegen && tsc",
    "lint": "pnpm biome && pnpm prettier && pnpm typecheck",
    "lint:fix": "pnpm biome:fix && pnpm prettier:fix && pnpm typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-router/fs-routes": "7.6.2",
    "@react-router/node": "7.6.2",
    "@react-router/serve": "7.6.2",
    "isbot": "5.1.28",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@react-router/dev": "7.6.2",
    "@tailwindcss/vite": "4.1.10",
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "17.6.3",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "**/*.{css,js,ts,cjs,mjs,cts,mts,jsx,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "**/*.{md,html,yaml,yml,json,jsonc}": [
      "prettier --write"
    ]
  }
}
